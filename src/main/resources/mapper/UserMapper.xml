<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.city.forum.mapper.UserMapper">
    <!--根据帐号查询出用户实体对象-->
    <select id="selectByUserAccount" resultType="com.city.forum.model.entity.User">
        select * from user where user_account = #{user_acount}
    </select>
</mapper>